CREATE OR REPLACE TABLE `first-case-study-485421.cyclistic_tripdata_2025.trips_2025_types_of_bikes` AS
(
SELECT
  EXTRACT(MONTH FROM started_at ) AS Month,
  rideable_type,
  member_casual,
  COUNT(*) AS total_rides
FROM `first-case-study-485421.cyclistic_tripdata_2025.trips_2025_cleaned`
GROUP BY Month,rideable_type, member_casual
ORDER BY Month
)
